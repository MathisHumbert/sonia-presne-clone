{
  "name": "creative-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "webpack server --progress --config webpack.config.development.js",
    "sync": "browser-sync start --proxy http://localhost:3000 --files \"dist/**/*\" --no-open --no-notify --port 4000",
    "dev": "concurrently --kill-others \"yarn backend:development\" \"yarn frontend:development\" \"yarn sync\""
  },
  "author": "Mathis Humbert",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "babel-loader": "^9.1.2",
    "browser-sync": "^2.29.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-webpack-plugin": "^4.0.1",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.12.1",
    "image-minimizer-webpack-plugin": "^3.8.2",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "include-media": "^2.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "normalize-wheel": "^1.0.1",
    "postcss-loader": "^7.3.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "terser-webpack-plugin": "^5.3.8",
    "three": "^0.153.0",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@prismicio/client": "^7.1.0",
    "@prismicio/helpers": "^2.3.9",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "errorhandler": "^1.5.1",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "2.6.7",
    "nodemon": "^2.0.22",
    "prefix": "^1.0.0",
    "pug": "^3.0.2",
    "ua-parser-js": "^1.0.35"
  }
}
